var address = '';
var instance = ComputationService.at(address);
var accounts = web3.eth.accounts;
web3.eth.defaultAccount = accounts[0];

instance.register(0,accounts[0],{from:accounts[0], gas: 400000, value: web3.toWei(0.01, "ether")})

instance.compute("123", "534543", 0, {from:accounts[0], gas: 400000, value: web3.toWei(0.01, "ether")})

instance.result() // insert my_id from Oraclize

web3.fromWei(web3.eth.getBalance(accounts[0]), "ether");
